{
  "name": "openrouter-mcp",
  "version": "1.1.0",
  "description": "Production-grade access to 200+ AI models via OpenRouter with resilient caching and error handling.",
  "keywords": ["ai", "llm", "openrouter", "mcp", "models"],
  "mcpServers": {
    "openrouter": {
      "command": "npx",
      "args": [
        "ts-node",
        "--esm",
        "openrouter-mcp.ts"
      ]
    }
  },
  "contextFileName": "GEMINI.md",
  "settings": [
    {
      "name": "OpenRouter API Key",
      "description": "Your API key from openrouter.ai",
      "envVar": "OPENROUTER_API_KEY",
      "sensitive": true
    }
  ]
}
